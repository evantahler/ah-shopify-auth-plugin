    "nonce": "^1.0.4",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/validator": "^12.0.1",
    "actionhero": "^21.0.0",
    "jest": "^24.9.0",
    "pg": "^7.14.0",
    "prettier": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^24.2.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "peerDependencies": {
    "actionhero": ">=21.0.0",
    "cookie": "^0.4.0",
    "crypto": "^1.0.1",
